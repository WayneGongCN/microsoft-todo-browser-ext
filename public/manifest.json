{
  "name": "Microsoft To Do chrome extension",
  "version": "0.0.1",
  "description": "Microsoft To Do chrome extension",
  "permissions": ["identity", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "./icons/todo-16.png",
    "24": "./icons/todo-24.png",
    "32": "./icons/todo-32.png",
    "48": "./icons/todo-48.png",
    "128": "./icons/todo-128.png"
  },
  "manifest_version": 2
}
